{
  "name": "content-portal",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "turbo dev",
    "dev:portal": "turbo dev --filter=portal",
    "dev:katalk": "turbo dev --filter=katalk-analyzer",
    "dev:menhera": "turbo dev --filter=menhera-analyzer",
    "dev:mbti": "turbo dev --filter=mbti-analyzer",
    "dev:relationship": "turbo dev --filter=relationship-analyzer",
    "dev:mockexam": "turbo dev --filter=mockexam-analyzer",
    "dev:bestfriend": "turbo dev --filter=bestfriend-analyzer",
    "dev:greenlight": "turbo dev --filter=greenlight-analyzer",
    "dev:chattype": "turbo dev --filter=chattype-analyzer",
    "dev:balance": "turbo dev --filter=balance-analyzer",
    "dev:character": "turbo dev --filter=character-analyzer",
    "build": "turbo build",
    "build:pages": "turbo build && node scripts/merge-builds.js",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
